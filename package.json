{
  "name": "cloud-tutorials-frontend",
  "version": "1.1.0",
  "private": false,
  "scripts": {
    "uberstart": "cd apps/mk-ui-frontend && BASE_PATH=http://localhost:8000 npm run start:dev",
    "build": "webpack --config config/prod.webpack.config.js",
    "analyze": "NODE_ENV=production webpack --config config/prod.webpack.config.js --env analyze=true",
    "build:prod": "NODE_ENV=production webpack --config config/prod.webpack.config.js",
    "deploy": "npm-run-all build:prod lint test",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint config src",
    "lint:js:fix": "eslint config src --fix",
    "lint:sass": "stylelint 'src/**/*.scss' --config .stylelintrc.json",
    "nightly": "npm run deploy",
    "prod": "NODE_ENV=production webpack serve --config config/dev.webpack.config.js",
    "server:ctr": "node src/server/generateServerKey.js",
    "start": "NODE_ENV=development webpack serve --config config/dev.webpack.config.js",
    "start:backend": "node src/backend/server.js",
    "test": "jest --verbose",
    "verify": "npm-run-all build lint test"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/stories/*"
    ],
    "coverageDirectory": "./coverage/",
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "roots": [
      "<rootDir>/src/"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ]
  },
  "transformIgnorePatterns": [
    "/node_modules/(?!@redhat-cloud-services)"
  ],
  "dependencies": {
    "@babel/runtime": "7.12.13",
    "@patternfly/react-core": "4.90.2",
    "@patternfly/react-table": "4.20.15",
    "@redhat-cloud-services/frontend-components": "^3.0.3",
    "@redhat-cloud-services/frontend-components-notifications": "^3.0.3",
    "@redhat-cloud-services/frontend-components-utilities": "^3.0.2",
    "@rh-uxd/appservices-patternfly-core": "^1.0.5",
    "classnames": "2.2.6",
    "client-oauth2": "^4.3.3",
    "detect-browser": "^5.2.0",
    "https-browserify": "^1.0.0",
    "i18next": "11.6.0",
    "i18next-xhr-backend": "1.5.1",
    "js-cookie": "^2.2.1",
    "lodash.get": "^4.4.2",
    "mustache": "^4.1.0",
    "path-browserify": "^1.0.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-redux": "5.0.6",
    "react-router-dom": "5.2.0",
    "redux": "4.0.5",
    "redux-logger": "3.0.6",
    "redux-promise-middleware": "5.1.1",
    "redux-thunk": "^2.3.0",
    "sha.js": "^2.4.11",
    "stream-http": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "7.12.13",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.12.13",
    "@babel/preset-env": "7.12.13",
    "@babel/preset-flow": "7.12.13",
    "@babel/preset-react": "7.12.13",
    "@redhat-cloud-services/eslint-config-redhat-cloud-services": "^1.0.0",
    "@redhat-cloud-services/frontend-components-config": "3.0.0-beta18",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "asciidoctor": "^2.2.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "26.6.3",
    "babel-plugin-dual-import": "1.2.1",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-transform-imports": "^2.0.0",
    "body-parser": "^1.19.0",
    "copy-webpack-plugin": "^7.0.0",
    "cross-fetch": "^3.0.6",
    "css-loader": "5.0.1",
    "enzyme": "3.11.0",
    "enzyme-to-json": "3.6.1",
    "eslint": "7.19.0",
    "eslint-loader": "4.0.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.3",
    "npm-run-all": "4.1.5",
    "prop-types": "15.7.2",
    "react-i18next": "8.3.8",
    "sequelize": "^6.5.0",
    "sqlite3": "^5.0.1",
    "stylelint": "13.9.0",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-scss": "3.18.0",
    "webpack": "5.20.0",
    "webpack-bundle-analyzer": "4.4.0",
    "webpack-cli": "4.5.0"
  },
  "insights": {
    "appname": "cloud-tutorials"
  },
  "workspaces": [
    "apps/mk-ui-frontend",
    "apps/strimzi-ui"
  ]
}
